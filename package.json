{
  "name": "react-virtual-infinite-list",
  "version": "0.1.1",
  "description": "Lightweight infinite scroll with pagination (page/cursor), virtual list, grid layout and window scroll support for React",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "prepublishOnly": "npm run build",
    "release": "npm run build && npm publish --access public"
  },
  "keywords": [
    "react",
    "infinite-scroll",
    "pagination",
    "virtual-list",
    "virtualization",
    "lazy-load",
    "cursor-pagination",
    "window-scroll",
    "grid-layout",
    "react-hooks"
  ],
  "author": "Hoang Nguyen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nguyenduyhoang35/react-virtual-infinite-list.git"
  },
  "bugs": {
    "url": "https://github.com/nguyenduyhoang35/react-virtual-infinite-list/issues"
  },
  "homepage": "https://github.com/nguyenduyhoang35/react-virtual-infinite-list#readme",
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
